<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="INFO">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="warn">
      <AppenderRef ref="Console"/>
    </Root>
    
	<Logger name="nl.tudelft.serg.evosql" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
	<Logger name="genetic" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>

  </Loggers>
</Configuration>